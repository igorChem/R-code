# Anova das análises do ACV dos calçados 

#=================================================================================

# 1. First things

# setting working directory-------------------------------------------------------

setwd("~/r/congACV")

# load packages 

library(lattice)

#================================================================================

# 2. Distribuições para as variáveis de impacto-----------------------------------

# 2.1 Lona de  jeans

# 2.1.1 PEAP --------------------------------------------------------------------

aPEAP <-rnorm(10000,mean=0.025,sd=0.071)

# 2.1.2 PEM ----------------------------------------------------------------------

aPEM <-rnorm(10000,mean=0.117,sd=0.26)

# 2.1.3 PTH-----------------------------------------------------------------------

aPTH <-rnorm(10000,mean=0.219,sd=0.077)

# 2.2 Lona de PU 

# 2.2.1 PEAP --------------------------------------------------------------------

bPEAP <-rnorm(10000,mean=0.039,sd=0.077)

# 2.2.2 PEM --------------------------------------------------------------------

bPEM <-rnorm(10000,mean=0.16,sd=0.27)

# 2.2.3 PTH --------------------------------------------------------------------

bPTH <-rnorm(10000,mean=0.1162,sd=0.048)

# 2.3 Lona de jeans reciclada 

# 2.3.1 PEAP --------------------------------------------------------------------

cPEAP <-rnorm(10000,mean=0.024,sd=0.32)

# 2.3.2 PEM --------------------------------------------------------------------

cPEM <-rnorm(10000,mean=0.098,sd=1.17)

# 2.3.3 PTH --------------------------------------------------------------------

cPTH <-rnorm(10000,mean=0.054,sd=0.027)

#=================================================================================

#3. Plots de distribuição 

dat1 <-data.frame(aPEM,bPEM,cPEM) 

densityplot(~aPEM+bPEM+cPEM,plot.points=F,auto.key=list(space="right"))

#=================================================================================

dat2 <-data.frame(aPEAP,bPEAP,cPEAP) 

densityplot(~aPEAP+bPEAP+cPEAP,plot.points=F,auto.key=list(space="right"))

#=================================================================================

dat3 <-data.frame(aPTH,bPTH,cPTH) 

densityplot(~aPTH+bPTH+cPTH,plot.points=F,auto.key=list(space="right"))

#=================================================================================

PTH <-c(aPTH,bPTH,cPTH)


